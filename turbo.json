{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "release/**", "build/**"]
    },
    "build:rn": {
      "dependsOn": ["^build:rn"]
    },
    "extract": {
      "dependsOn": ["^extract"],
      "outputs": ["out/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    },
    "test:staged": {
      "dependsOn": ["^test:staged"],
      "outputs": ["coverage/**"]
    }
  }
}
